CREATE TABLE `User` (
  `USR_ID` int(11) NOT NULL AUTO_INCREMENT,
  `USR_FIRST_NAME` varchar(45) COLLATE utf8_unicode_ci NOT NULL,
  `USR_LAST_NAME` varchar(45) COLLATE utf8_unicode_ci NOT NULL,
  `USR_EMAIL` varchar(50) COLLATE utf8_unicode_ci NOT NULL,
  `USR_PASSWORD` varchar(25) COLLATE utf8_unicode_ci DEFAULT ' ',
  PRIMARY KEY (`USR_ID`),
  UNIQUE KEY `USR_ID` (`USR_ID`),
  UNIQUE KEY `USR_EMAIL_UNIQUE` (`USR_EMAIL`)
) ENGINE=MyISAM AUTO_INCREMENT=3 DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci;

CREATE TABLE `MadLib` (
  `MadLib_ID` int(11) NOT NULL AUTO_INCREMENT,
  `USER_ID` int(11) NOT NULL,
  `Plural_Noun_One` varchar(45) COLLATE utf8_unicode_ci NOT NULL,
  `Plural_Noun_Two` varchar(45) COLLATE utf8_unicode_ci NOT NULL,
  `Adjective_One` varchar(45) COLLATE utf8_unicode_ci NOT NULL,
  `Noun_One` varchar(45) COLLATE utf8_unicode_ci NOT NULL,
  `Adjective_Two` varchar(45) COLLATE utf8_unicode_ci NOT NULL,
  `Adjective_Three` varchar(45) COLLATE utf8_unicode_ci NOT NULL,
  `Verb` varchar(45) COLLATE utf8_unicode_ci NOT NULL,
  `Body_Part` varchar(45) COLLATE utf8_unicode_ci NOT NULL,
  `Adjective_Four` varchar(45) COLLATE utf8_unicode_ci NOT NULL,
  `Number` int(11) NOT NULL,
  `Noun_Two` varchar(45) COLLATE utf8_unicode_ci NOT NULL,
  PRIMARY KEY (`MadLib_ID`),
  UNIQUE KEY `MadLib_ID` (`MadLib_ID`),
  UNIQUE KEY `MadLib_ID_UNIQUE` (`MadLib_ID`),
  KEY `MadLib_USER_ID` (`USER_ID`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci;
